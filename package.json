{
  "name": "react-appid-ui",
  "version": "0.1.0",
  "description": "Demo React AppId user interface",
  "license": "APACHE 2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelsteven/react-appid-ui.git"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "i18next": "^21.6.4",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "install": "^0.13.0",
    "jwt-decode": "^3.1.2",
    "moment": "2.29.1",
    "npm": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.22.5",
    "react-i18next": "^11.15.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "reakit": "^1.3.11",
    "typescript": "^4.5.4",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "PORT=8080 react-scripts start",
    "start:mock": "cross-env REACT_APP_API=mock react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test",
    "test:watch": "cross-env react-scripts test",
    "lint": "eslint --max-warnings 0 src",
    "check-types": "tsc",
    "format": "prettier --write '**/*.@(js|json|ts|tsx)'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "pre-commit": "npm-run-all lint check-types test"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-var": "error"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 safari version",
      "last 2 edge version",
      "last 2 firefox version",
      "last 2 ios_saf version",
      "last 2 and_chr version",
      "last 2 samsung version",
      "last 2 and_ff version",
      ">2% in CA",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version",
      "last 1 firefox version",
      "last 1 edge version"
    ]
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          [
            "react-remove-properties"
          ],
          {
            "properties": [
              "data-testid"
            ]
          }
        ]
      }
    }
  }
}
